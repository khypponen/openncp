		<!-- Environment config (These are default values. Override these props
			in settings.xml) -->
		<!-- Default empty build number and id, is overridden by CI server -->
		<BUILD_NUMBER></BUILD_NUMBER>
		<BUILD_ID></BUILD_ID>
		<integrationtest.targetUrl>http://localhost:8080</integrationtest.targetUrl>
		<!-- script to run for remote deployment on integration test server, should
			be under src/scripts/ -->
		<integrationtest.deployScript>--</integrationtest.deployScript>
		<!-- username to use for remote deployment on integration test server -->
		<integrationtest.targetHostUser>--</integrationtest.targetHostUser>
		<!-- user private key to use for remote deployment on integration test
			server -->
		<integrationtest.targetHostUserSSHKey>--</integrationtest.targetHostUserSSHKey>
		<!-- Application config properties external override, ignored if it does
			not exist -->
		<epsos-web-application-properties>${basedir}/src/main/java/se/sb/epsos/web/EpsosWebApplication_en.properties</epsos-web-application-properties>
		<applicationConfig>${basedir}/src/main/config/${user.name}-application.properties</applicationConfig>
		<!-- Path to xmlR resources needed by gnomgon display tool, generated with
			tsam-export tools -->
		<displayTool.xmlResourcePath>${basedir}/src/main/resources/EpsosRepository</displayTool.xmlResourcePath>
		<epsos-internationalsearch-config-path>${basedir}/src/main/resources/config</epsos-internationalsearch-config-path>
		<client-connector-wsdl-url>http://localhost:8085/epsos-client-connector/services/ClientConnectorService?wsdl</client-connector-wsdl-url>
	</properties>

